Procedure TtestForm1.InfoGridWrite;
Var
    F : TextFile;
Begin
   AssignFile(F,'InfoGridSave.txt');
   Rewrite(F);
   DM.Info.First;
   while not DM.Info.EOF do
     begin
        Writeln(F,DM.Info.FieldByName('No').AsInteger);
        Writeln(F,DM.Info.FieldByName('Value').AsInteger);
        Writeln(F,DM.Info.FieldByName('Name').AsString);
        Writeln(F,DM.Info.FieldByName('ConscidaNo').AsInteger);
        DM.Info.Next;
     end;
   CloseFile(F);
End;

Procedure TtestForm1.InfoGridRead;
Var
    F : TextFile;
    I1,I2,I3 : Integer;
    S : String;
Begin
   AssignFile(F,'InfoGridSave.txt');
   If FileExists('InfoGridSave.txt') Then Reset(F) else Exit;
   while not EOF(F) do
     begin
        Readln(F,I1);
        Readln(F,I2);
        Readln(F,S);
        Readln(F,I3);

